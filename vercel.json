{
	"version": 2,
	"builds": [
		{
			"src": "htmx_channels/wsgi.py",
			"use": "@vercel/python",
			"config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
		},
		{
			"src": "build_files.sh",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "static_root"
			}
		}
	],
	"routes": [
		{
			"src": "/static_root/(.*)",
			"dest": "/static_root/$1"
		},
		{
			"src": "/(.*)",
			"dest": "htmx_channels/wsgi.py"
		}
	]
}
